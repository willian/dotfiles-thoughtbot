export CDPATH=.:~:~/Projects:~/Projects/Toptal:~/GitHub:/vagrant
export LC_ALL="$LANG"
export PATH="$HOME/.rbenv/bin:./bin:$HOME/local/node/npm/bin:/usr/local/sbin:$PATH"

# load rbenv if available
if which rbenv &>/dev/null ; then
  eval "$(rbenv init - --no-rehash zsh)"
fi

eval "$(docker-machine env default --shell=zsh)"
