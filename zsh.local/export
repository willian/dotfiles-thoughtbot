export CDPATH=.:~:~/Projects:~/Projects/PrintBear:~/Projects/Toptal:~/GitHub:/vagrant
export LC_ALL="$LANG"
export GOPATH=$HOME/.go
export TERM=xterm-256color
PATH="./bin:$HOME/local/node/npm/bin:/usr/local/sbin:/Applications/VMware Fusion.app/Contents/Library:$GOPATH/bin:$HOME/.yarn/bin:$PATH"

if which vmrun &>/dev/null ; then
  if [[ `vmrun list` =~ default ]] && which docker-machine &>/dev/null ; then
    eval "$(docker-machine env default --shell=zsh)"
  fi
fi

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
